msgid "Octal"
msgstr "Octal"

#: C/mate-calc.xml:234(para)
msgid "8"
msgstr "8"

#: C/mate-calc.xml:237(para)
msgid "Decimal"
msgstr "Decimal"

#: C/mate-calc.xml:238(para)
msgid "10"
msgstr "10"

#: C/mate-calc.xml:241(para)
msgid "Hexadecimal"
msgstr "Hexadecimal"

